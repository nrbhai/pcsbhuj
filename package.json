{
  "name": "pioneer-computer-services",
  "version": "1.0.0",
  "description": "Website for Pioneer Computer Services, Bhuj",
  "main": "server/index.js",
  "scripts": {
    "client": "cd client && npm start",
    "server": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-all": "npm install && npm run install-client",
    "vercel-build": "npm run install-client && npm run build"
  },
  "keywords": ["computers", "laptops", "cctv", "bhuj"],
  "author": "Pioneer Computer Services",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
