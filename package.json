{
  "name": "pioneer-computer-services",
  "version": "1.0.0",
  "description": "Website for Pioneer Computer Services, Bhuj",
  "main": "server/index.js",
  "scripts": {
    "client": "cd client && npm start",
    "server": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-all": "npm install && npm run install-client"
  },
  "keywords": [
    "computers",
    "laptops",
    "cctv",
    "bhuj"
  ],
  "author": "Pioneer Computer Services",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "nodemailer": "^7.0.11",
    "react-helmet-async": "^2.0.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "html-webpack-plugin": "^5.6.4",
    "lodash": "^4.17.21",
    "style-loader": "^4.0.0"
  }
}
